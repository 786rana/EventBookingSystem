@model List<EventBookingSystem.Models.MarriageHall>
<h2>
    <a href="/MarriageHall/Marriage"><input type="button" value="Add MarriageHall" class="btn btn-primary" /></a>
</h2>
<table class="table table-responsive">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Phone</td>
            <td>Location</td>
            <td>Detail</td>
            <td>Actions:</td>

        </tr>
    </thead>
    <tbody>
        @foreach(var Ebs in Model)
        {
            <tr>
                <td>@Ebs.Id</td>
                <td>@Ebs.Name</td>
                <td>@Ebs.Phone</td>
                <td>@Ebs.Location</td>
                <td>@Ebs.Details</td>
                <td>
                    <a href="/MarriageHall/Edit?Id=@Ebs.Id"><input type="button" value="Edit" class="btn  btn-primary"/></a>
                    <a href="/MarriageHall/Delete?Id=@Ebs.Id"><input type="button" value="Delete" class="btn btn-danger"/></a>
                </td>
            </tr>
        }
    </tbody>
</table>